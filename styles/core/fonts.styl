// Proxima Nova Soft

@font-face {
    font-family: 'Proxima Nova Soft';
    src: url('../fonts/proxima-nova-soft-black-webfont.eot');
    src: url('../fonts/proxima-nova-soft-black-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/proxima-nova-soft-black-webfont.woff') format('woff'),
         url('../fonts/proxima-nova-soft-black-webfont.ttf') format('truetype'),
         url('../fonts/proxima-nova-soft-black-webfont.svg#proximanovasoft-buploadedfile') format('svg');
    font-weight: light;
    font-style: normal;
}

