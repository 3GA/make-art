{ "key": "",
  "name": "Kano Connect",
  "manifest_version": 2,
  "version": "0.3",
  "minimum_chrome_version": "39.0.2140.0",
  "app": {
    "background": {
      "scripts": [ "main.js" ]
    }
  },
  "icons": {
    "16": "16.png",
    "128": "128.png"
  },
  "permissions": [
    "http://*.kano.me/",
    "serial",
    "usb"
  ],

  "externally_connectable": {
    "ids": "*",
    "matches": ["*://*.kano.me/*"]
  }
}
